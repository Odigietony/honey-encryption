                    <li>
                        <a href="@Url.Action("Register", "Account")" class="btn btn-simple">Register</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Login", "Account")" class="btn btn-simple">Login</a>
                    </li>
                    <li>
